{
  "name": "Malam - Import eHarmony Hours",
  "version": "1.0",
  "description": "Imports NMI hours from eHarmony Excel to Malam online interface.",
  "permissions": ["storage", "declarativeContent", "activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/malam16.png",
    "32": "images/malam32.png",
    "48": "images/malam48.png",
    "128": "images/malam128.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}